{
    "name": "obsidian-bible-callout",
    "version": "1.0.0",
    "description": "Callout Bible verses in Obsidian",
    "main": "main.js",
    "scripts": {
        "dev": "node scripts/esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node scripts/esbuild.config.mjs production",
        "version": "node scripts/version-bump.mjs && git add manifest.json versions.json",
        "deploy": "sh scripts/install.sh",
        "test": "jest"
    },
    "keywords": [
        "obsidian",
        "bible",
        "callout",
        "plugin"
    ],
    "author": "Stephen Ullom",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.29.0",
        "builtin-modules": "3.3.0",
        "esbuild": "0.17.3",
        "jest": "^27.0.0",
        "obsidian": "latest",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "ts-jest": "^27.1.5",
        "@types/jest": "^27.0.0"
    }
}
